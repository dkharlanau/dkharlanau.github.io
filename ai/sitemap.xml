---
layout: null
permalink: /ai/sitemap.xml
sitemap: false
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
        xmlns:xhtml="http://www.w3.org/1999/xhtml"
        xmlns:ai="https://dkharlanau.github.io/ns/ai">
  {% assign ai_resources = "/LLM.txt|Interaction playbook|text/plain|weekly,/ai/resume.yml|Public resume dataset|application/yaml|monthly,/ai/resume.json|Resume dataset (JSON)|application/json|weekly,/ai/principles.json|Consulting principles|application/json|monthly,/ai/recommendations.json|Recommendations feed|application/json|monthly,/ai/home.json|Home snapshot|application/json|weekly" | split: "," %}
  {% for resource in ai_resources %}
    {% assign parts = resource | split: "|" %}
    <url>
      <loc>{{ parts[0] | absolute_url }}</loc>
      <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
      <changefreq>{{ parts[3] }}</changefreq>
      <priority>0.7</priority>
      <ai:title>{{ parts[1] }}</ai:title>
      <ai:format>{{ parts[2] }}</ai:format>
      {% if parts[0] == "/ai/resume.yml" %}
      <xhtml:link rel="alternate" hreflang="en" href="{{ '/ai/resume.json' | absolute_url }}" />
      {% endif %}
    </url>
  {% endfor %}
</urlset>
